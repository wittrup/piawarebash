
# You need to specify the shell, to ensure that $RANDOM is available.
SHELL=/bin/bash
# At 05:(random) on Monday:
0 5 * * 1 sleep $((RANDOM*3600/32768)) && ${HOME}/bin/aptgetauto.sh >> aptgetauto.log
 
